{
  "name": "monk-lang",
  "displayName": "Monk Lang",
  "description": "Language support for Monk, a programming language for the modern age",
  "publisher": "monkfromearth",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.54.0"
  },
  "author": {
    "name": "Sameer Khan",
    "email": "hi@monkfrom.earth",
    "url": "https://github.com/monkfromearth"
  },
  "pricing": "Free",
  "license": "MIT",
  "icon": "icons/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/monkfromearth/monk-lang"
  },
  "categories": [
    "Programming Languages"
  ],
  "bugs": {
    "url": "https://github.com/monkfromearth/monk-lang/issues",
    "email": "hi@monkfrom.earth"
  },
  "homepage": "https://github.com/monkfromearth/monk-lang/blob/main/README.md",
  "contributes": {
    "languages": [
      {
        "id": "monk",
        "aliases": [
          "Monk",
          "monk"
        ],
        "extensions": [
          ".monk",
          ".mnk"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "monk",
        "scopeName": "source.monk",
        "path": "./syntaxes/monk.tmLanguage.json"
      }
    ],
    "iconThemes": [
      {
        "id": "monk-icons",
        "label": "Monk Icons",
        "path": "./monk-icon-theme.json"
      }
    ]
  }
}